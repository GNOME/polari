#. commands that would be nice to support:
#.
#. AWAY: N_("/AWAY [<message>] — sets or unsets away message"),
#. LIST: N_("/LIST [<channel>] — lists stats on <channel>, or all channels on the server"),
#. MODE: "/MODE <mode> <nick|channel> — ",
#. NOTICE: N_("/NOTICE <nick|channel> <message> — sends notice to <nick|channel>"),
#. OP: N_("/OP <nick> — gives channel operator status to <nick>"),
#.
#.
#: src/ircParser.js:31
msgid ""
"/CLOSE [<channel>] [<reason>] — closes <channel>, by default the current one"
msgstr ""
"/CLOSE [<channel>] [<reason>] — lokar <channel>, að sjálfgefnu sá núverandi"

#: src/ircParser.js:32
msgid ""
"/HELP [<command>] — displays help for <command>, or a list of available "
"commands"
msgstr ""
"/HELP [<command>] — sýna hjálp fyrir <command>, eða lista yfir tiltækar "
"skipanir"

#: src/ircParser.js:33
msgid ""
"/INVITE <nick> [<channel>] — invites <nick> to <channel>, or the current one"
msgstr "/INVITE <nick> [<channel>] — býður <nick> í <channel>, eða núverandi"

#: src/ircParser.js:34
msgid "/JOIN <channel> — joins <channel>"
msgstr "/JOIN <channel> — tengist <channel>"

#: src/ircParser.js:35
msgid "/KICK <nick> — kicks <nick> from current channel"
msgstr "/KICK <nick> — vísar út <nick> af núverandi spjallrás"

#: src/ircParser.js:36
msgid "/ME <action> — sends <action> to the current channel"
msgstr "/ME <action> — sendir <action> til rásar"

#: src/ircParser.js:37
msgid "/MSG <nick> [<message>] — sends a private message to <nick>"
msgstr "/MSG <nick> [<message>] — sendir einkaskilaboð til <nick>"

#: src/ircParser.js:38
msgid "/NAMES — lists users on the current channel"
msgstr "/NAMES — gefur lista yfir notendur á rásinni"

#: src/ircParser.js:39
msgid "/NICK <nickname> — sets your nick to <nickname>"
msgstr "/NICK <nickname> — breytir gælunafni þínu í <nickname>"

#: src/ircParser.js:40
msgid ""
"/PART [<channel>] [<reason>] — leaves <channel>, by default the current one"
msgstr ""
"/PART [<channel>] [<reason>] — yfirgefur <channel>, sjálfgefið þá núverandi"

#: src/ircParser.js:41
msgid "/QUERY <nick> — opens a private conversation with <nick>"
msgstr "/QUERY <nick> — opnar einkaspjall við <nick>"

#: src/ircParser.js:42
msgid "/QUIT [<reason>] — disconnects from the current server"
msgstr "/QUIT [<reason>] — aftengdist núverandi netþjón"

#: src/ircParser.js:43
msgid "/SAY <text> — sends <text> to the current room/contact"
msgstr "/SAY <text> — sendir <text> í núverandi spjallrými/tenglar"

#: src/ircParser.js:44
msgid "/TOPIC <topic> — sets the topic to <topic>, or shows the current one"
msgstr "/TOPIC <topic> — breytið umræðuefni í <topic>, eða sýnið núverandi"

#: src/ircParser.js:45
msgid "/WHOIS <nick> — requests information on <nick>"
msgstr "/WHOIS <nick> — kallið fram upplýsingar um <nick>"

#: src/ircParser.js:48
msgid "Unknown command — try /HELP for a list of available commands"
